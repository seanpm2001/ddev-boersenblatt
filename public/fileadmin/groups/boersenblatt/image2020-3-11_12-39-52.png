<script>
function initNumberedHeadings() {
    if ("undefined" == typeof(tinyMCE))
        setTimeout(initNumberedHeadings, 1000);
	
    else {
		var createStyle = function(move, format, start) {
			var result = ".numbered-headings-custom { counter-reset: c1 " + (start-1) + "; } ";
			
			for (var i=1; i<7; i++) {
				result += ".numbered-headings-custom h" + (i+move) + ":before { counter-increment: c" + i + "; content: ";
				var levels = [];
				for (var j=0; j<i; j++) {
					var lastSpace = j+1 == i ? " " : "";
					levels.push("counter(c" + (j+1) + ", " + format + ") '." + lastSpace + "'");
				}
				result += levels.join(" ") + ";} ";
				
				result += ".numbered-headings-custom h" + (i+move) + " { counter-reset: c" + (i+1) + "; } ";
			}
			
			return result;
		};
		
		var parseParams = function(value) {
			var result = {"start-numbering-at": "h1", "number-format": "decimal", "start-numbering-with": "1"};
			var parts = value.split("|");
			
			for (var i=0; i<parts.length; i++) {
				var paramParts = parts[i].split("=");
				result[paramParts[0]] = paramParts[1];
			}
			
			return result;
		};
		
		tinyMCE.on('addeditor', function( event ) {
			event.editor.on('nodechange', function (e) {
				try {
					var macros = tinymce.activeEditor.dom.select(".wysiwyg-macro[data-macro-name='numberedheadings']");
					if (0 != macros.length) {
						var macro = $(macros.pop());
						var config = macro.data("macro-parameters");
						var styleTag = macro.find("style");
						
						if (0 != styleTag.length && lastNHConfig == config) return;
						lastNHConfig = config;
						
						var params = parseParams(config);
						var move = parseInt(params["start-numbering-at"].slice(1)) - 1;
						
						macro.addClass("numbered-headings-custom");
						styleTag.remove();

						$("<style type='text/css'>" + createStyle(move, params["number-format"], parseInt(params["start-numbering-with"])) +"</style>").prependTo(macro);
					}
				
				} catch(e) { console.log(e); }
			});
		}, true );
	}
}

var lastNHConfig;
$(function() {
	initNumberedHeadings();
});
</script>
        
